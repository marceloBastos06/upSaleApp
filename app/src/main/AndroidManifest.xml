<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.upsale.upsaleapp">

    <user-sdk android:minSdkVersion="16" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.Splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity">

        </activity>

        <activity
            android:name=".activity.PrimeiraTelaActivity"
            android:label="@string/title_activity_primeira_tela"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".activity.FinanciasActivity" />
        <activity android:name=".activity.GraficoActivity" />
        <activity android:name=".activity.EstoqueActivity"></activity>
    </application>

</manifest>
